{
  "name": "sunseekerx-notebook",
  "version": "2.0.0",
  "private": true,
  "description": "sunseekerx's notebook.",
  "license": "MIT",
  "type": "module",
  "scripts": {
    "dev": "vuepress dev docs",
    "docs:build": "vuepress build docs",
    "docs:clean-dev": "vuepress dev docs --clean-cache",
    "docs:dev": "vuepress dev docs",
    "gc": "git add -A && git-cz && git pull && git push",
    "lint:lint-staged": "lint-staged",
    "lint:prettier": "prettier --write --loglevel warn \"./**/*.{js,json,tsx,css,less,scss,vue,html,md}\"",
    "pkg:sort": "npx sort-package-json",
    "serve": "vuepress dev docs",
    "prepare": "husky install"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "prettier --write"
    ],
    "{!(package)*.json,*.code-snippets,.!(browserslist)*rc}": [
      "prettier --write--parser json"
    ],
    "package.json": [
      "prettier --write"
    ],
    "*.md": [
      "prettier --write"
    ]
  },
  "devDependencies": {
    "@vue/repl": "^2.6.1",
    "@vuepress/client": "2.0.0-beta.67",
    "@vuepress/plugin-google-analytics": "2.0.0-beta.67",
    "chart.js": "^4.4.0",
    "cross-env": "^7.0.3",
    "echarts": "^5.4.3",
    "husky": "^8.0.0",
    "katex": "^0.16.9",
    "lint-staged": "^15.0.2",
    "mermaid": "^10.6.0",
    "prettier": "^3.0.3",
    "reveal.js": "^5.0.1",
    "vue": "3.3.7",
    "vuepress": "2.0.0-beta.67",
    "vuepress-plugin-search-pro": "2.0.0-beta.242",
    "vuepress-theme-hope": "2.0.0-beta.242"
  }
}
